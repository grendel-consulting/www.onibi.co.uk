{
  "name": "www.onibi.co.uk",
  "description": "Brochureware site for Onibi imprint",
  "author": {
    "name": "James Ramirez"
  },
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,mjs,json,ts,tsx,md,mdx,yml,astro,feature}\"",
    "synth": "turbo run synth",
    "browsers": "turbo run browsers",
    "e2e": "turbo run e2e",
    "ci": "turbo run ci"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 1 Firefox versions",
      "last 1 Safari versions"
    ]
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-gherkin": "3.1.3",
    "turbo": "2.5.8",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "sharp@<0.32.6": ">=0.32.6",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2"
    }
  }
}
