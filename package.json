{
  "name": "www.onibi.co.uk",
  "description": "Brochureware site for Onibi imprint",
  "author": "James Ramirez",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "synth": "turbo run synth",
    "format": "prettier  --ignore-path .gitignore --write \"**/*.{json,ts,tsx,md}\"",
    "ci": "turbo run ci"
  },
  "devDependencies": {
    "esbuild": "0.24.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "3.3.3",
    "turbo": "2.2.3"
  },
  "pnpm": {
    "overrides": {
      "axios": "1.7.7",
      "webpack-dev-middleware@<=5.3.3": ">=5.3.4",
      "braces@<3.0.3": ">=3.0.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "micromatch@<4.0.8": ">=4.0.8",
      "webpack@>=5.0.0-alpha.0 <5.94.0": ">=5.94.0",
      "body-parser@<1.20.3": ">=1.20.3",
      "send@<0.19.0": ">=0.19.0",
      "serve-static@<1.16.0": ">=1.16.0",
      "express@<4.20.0": ">=4.20.0",
      "path-to-regexp@<0.1.10": ">=0.1.10",
      "vite@>=5.2.0 <5.2.14": ">=5.2.14",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "cookie@<0.7.0": ">=0.7.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
